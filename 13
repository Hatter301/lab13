#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int x = 1, i = 0, n;
	int *a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int *)malloc(n * sizeof(int));
		for (i; i <= n - 1; i++) {
			a[i] = x;
			x += 2;
		}
		i = 0;
		for (i; i <= n - 1; i++)
			cout << a[i] << " ";
		free(a);
	}
  return 0;
}
