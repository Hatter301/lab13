#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i = 0, n;
	int *a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int *)malloc(n * sizeof(int));
		while (i != n) {
			cin >> a[i];
			i++;
		}
		i = 0;
		for (i; i<=n-1; i+=2) 
			cout << a[i] << " ";
		i = 0;
		if ((n % 2) == 0) {
			i = n - 1;
			for (i; i>=1; i -= 2)
				cout << a[i] << " ";
		}
		else { 
			i = n - 2;
			for (i; i >= 1; i -= 2)
				cout << a[i] << " ";
		}
		free(a);
	}
	return 0;
}
