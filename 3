#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i = 2, n,A,B,s;
	int *a;
	cin >> n>>A>>B;
	if (n <= 2)
		cout << "Данные введены некорректно";
	else {
		a = (int *)malloc(n * sizeof(int));
		a[0] = A;
		a[1] = B;
		s = a[0] + a[1];
		for (i; i <= n - 1; i++) {
			a[i] = s;
			s = s + a[i];
		}
		i = 0;
		for (i; i <= n - 1; i++)
			cout << a[i] << " ";
		free(a);
	}
	return 0;
}
